<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="562.0" prefWidth="444.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.whatsup.ClientController">
    <TextField fx:id="tfMessage" layoutX="48.0" layoutY="395.0" prefHeight="36.0" prefWidth="347.0" promptText="Escribe tu mensaje...">
      <font>
         <Font name="Century Gothic" size="12.0" />
      </font>
    </TextField>
   <TextField fx:id="keyValue" layoutX="55.0" layoutY="504.0" prefHeight="23.0" prefWidth="83.0" promptText="Llave">
      <font>
         <Font name="Century Gothic" size="13.0" />
      </font>
   </TextField>
   <Button fx:id="regresarButton" layoutX="48.0" layoutY="34.0" mnemonicParsing="false" onAction="#regresarButtonAction" prefHeight="27.0" prefWidth="49.0" text="&lt;&lt;" textFill="#aa4646">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </Button>
   <Label fx:id="contactLabel" layoutX="161.0" layoutY="30.0" text="WhatsUp" textFill="#aa4646">
      <font>
         <Font name="Century Gothic" size="28.0" />
      </font>
   </Label>
   <Button fx:id="plainSendButton" layoutX="54.0" layoutY="455.0" mnemonicParsing="false" onAction="#sendPlainText" prefHeight="27.0" prefWidth="98.0" style="-fx-background-color: #aa4646;" text="T.P." textFill="WHITE">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </Button>
   <Button layoutX="174.0" layoutY="455.0" mnemonicParsing="false" onAction="#ssButton" prefHeight="27.0" prefWidth="98.0" style="-fx-background-color: #aa4646;" text="C.S." textFill="WHITE">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </Button>
   <Button layoutX="292.0" layoutY="455.0" mnemonicParsing="false" onAction="#ssButton" prefHeight="27.0" prefWidth="98.0" style="-fx-background-color: #aa4646;" text="C.A." textFill="WHITE">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </Button>
   <Button fx:id="decoderButton" layoutX="297.0" layoutY="502.0" mnemonicParsing="false" onAction="#openDecoder" prefHeight="27.0" prefWidth="98.0" text="Descifrador" textFill="#aa4646">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </Button>
   <Button fx:id="confirmKey" layoutX="173.0" layoutY="502.0" mnemonicParsing="false" onAction="#sendSSMessage" prefHeight="27.0" prefWidth="98.0" text="Confirmar" textFill="#aa4646">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </Button>
   <TextArea fx:id="textArea" editable="false" layoutX="49.0" layoutY="88.0" prefHeight="287.0" prefWidth="347.0">
      <font>
         <Font name="Century Gothic" size="14.0" />
      </font>
   </TextArea>
</AnchorPane>
